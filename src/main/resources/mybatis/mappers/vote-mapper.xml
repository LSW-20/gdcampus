<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="voteMapper">

	<!-- 진행중-->
<resultMap id="voteResultMap" type="VoteDto">
    <id column="VOTE_ID" property="voteId"/>
    <result column="CREATE_USER" property="createUser"/>
    <result column="CREATE_DATE" property="createDate"/>
    <result column="VOTE_TITLE" property="voteTitle"/>
    <result column="VOTE_CONTENT" property="voteContent"/>
    <result column="VOTE_START" property="voteStart"/>
    <result column="VOTE_END" property="voteEnd"/>
    <result column="MODIFY_DATE" property="modifyDate"/>
    <result column="MODIFY_USER" property="modifyUser"/>
</resultMap>

	<select id="selectVoteListCount" resultType='_int'>
		select
					count(*)
			form
					t_vote
		 where 
		 			status = 'Y'		
	</select>

<select id="selectVoteList" resultMap="voteResultMap">
	select
				
</select>

</mapper>
